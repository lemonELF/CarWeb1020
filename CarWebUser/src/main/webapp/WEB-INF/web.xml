    <url-pattern>/RepDelServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>RepUpdServlet</servlet-name>
    <servlet-class>com.car.servlet.RepUpdServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RepUpdServlet</servlet-name>
    <url-pattern>/RepUpdServlet</url-pattern>
  </servlet-mapping>


  <session-config>
    <session-timeout>15</session-timeout>
  </session-config>




</web-app>

