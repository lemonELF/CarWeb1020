<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
  <welcome-file-list>
    <welcome-file>indexlogin.jsp</welcome-file>
  </welcome-file-list>

  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>regsel</servlet-name>
    <servlet-class>com.car.servlet.SelectServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>regsel</servlet-name>
    <url-pattern>/ServletServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>verservlet</servlet-name>
    <servlet-class>com.car.servlet.VerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>verservlet</servlet-name>
    <url-pattern>/verservlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>insservlet</servlet-name>
    <servlet-class>com.car.servlet.LInertServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>insservlet</servlet-name>
    <url-pattern>/LInsertServlet</url-pattern>
  </servlet-mapping>

  <servlet>
  <servlet-name>FogetServlet</servlet-name>
  <servlet-class>com.car.servlet.FogetServlet</servlet-class>
</servlet>
  <servlet-mapping>
    <servlet-name>FogetServlet</servlet-name>
    <url-pattern>/FogetServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ServletContent</servlet-name>
    <servlet-class>com.car.servlet.ServletContent</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletContent</servlet-name>
    <url-pattern>/ServletContent</url-pattern>
  </servlet-mapping>




  <servlet>
    <servlet-name>cancse</servlet-name>
    <servlet-class>com.car.servlet.CancelServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>cancse</servlet-name>
    <url-pattern>/cancelservlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>checkout</servlet-name>
    <servlet-class>com.car.servlet.CheckSession</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>checkout</servlet-name>
    <url-pattern>/checkout</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>loginsess</servlet-name>
    <servlet-class>com.car.servlet.SessionLogin</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>loginsess</servlet-name>
    <url-pattern>/loginsess</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>mycar</servlet-name>
    <servlet-class>com.car.servlet.CarServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>mycar</servlet-name>
    <url-pattern>/mycar</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>CountServlet</servlet-name>
    <servlet-class>com.car.servlet.CountServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CountServlet</servlet-name>
    <url-pattern>/CountServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>RepServlet</servlet-name>
    <servlet-class>com.car.servlet.RepServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RepServlet</servlet-name>
    <url-pattern>/RepServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>RepInsServlet</servlet-name>
    <servlet-class>com.car.servlet.RepInsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RepInsServlet</servlet-name>
